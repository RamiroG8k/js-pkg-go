{
  "name": "js-lib-go",
  "version": "0.2.0",
  "description": "This is the entry point for the js-lib-go library.",
  "author": "Ramiro M",
  "keywords": [
    "lib",
    "js"
  ],
  "files": [
    "dist/**/*"
  ],
  "license": "MIT",
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "test:coverage": "bun run test.ts coverage",
    "test:file": "bun run test.ts file",
    "build:dev": "bun run build.ts dev",
    "build:prod": "bun run build.ts prod",
    "publish:patch": "bun run publish.ts patch",
    "publish:minor": "bun run publish.ts minor",
    "publish:major": "bun run publish.ts major",
    "publish:info": "bun run publish.ts info"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "publishConfig": {
    "access": "public"
  }
}
